<?xml version="1.0" encoding="UTF-8"?>
<fof>
	<common>
		<container>
			<option name="factoryClass">FOF30\Factory\MagicFactory</option>
		</container>

		<dispatcher>
			<option name="defaultView">Contents</option>
		</dispatcher>

		<view name="Contents">
			<config>
				<option name="useConfigurationFile">1</option>
			</config>

			<toolbar task="browse">
				<button type="title" translate="1" icon="stack">COM_FOFCONTENT_TITLE_CONTENTS</button>
				<button type="new" />
				<button type="editList" />
				<button type="publishList" />
				<button type="unpublishList" />
				<button type="archiveList" />
				<button type="trash" />
			</toolbar>
		</view>
	</common>
</fof>
