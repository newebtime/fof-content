<?xml version="1.0" encoding="utf-8"?>
<form
	type="browse"
>
	<headerset>
		<header name="fofcontent_content_id" type="RowSelect" tdwidth="20" />
		<header name="enabled" type="Field" />
		<header name="title" type="Field" />
		<header name="access" type="Field" />
		<header name="created_by" type="Field" />
		<header name="language" type="Field" />
		<header name="created_on" type="Field" />
	</headerset>

	<fieldset name="items">
		<field name="fofcontent_content_id" type="SelectRow"/>
		<field name="enabled" type="Actions" show_archived="1" show_trash="1"/>
		<field name="title" type="Title" />
		<field name="access" type="AccessLevel" />
		<field name="created_by" type="User" show_id="0" show_email="0" show_username="0" show_avatar="0" show_link="true" />
		<field name="language" type="Language" />
		<field name="created_on" type="Calendar" />
	</fieldset>
</form>
